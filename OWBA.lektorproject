[project]
name = Offene Werkstatt Bamberg
url = https://owba.de/
locale = de_DE

[servers.production]
name = Production
enabled = yes
default = yes
target = path:///srv/http/de-owba

[servers.git-commit]
name = Git Commit
enabled = yes
target = cmd://git/add * && git commit -m 'Automerge changes' && git push
